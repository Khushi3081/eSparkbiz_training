                                <th>department</th>
                            </tr>
                        </thead>
                      <tbody>
                            <% for(let i=0;i< pref_data.length;i++) {%>
                                <tr>
                                    <td>
                                        <%= pref_data[i].id %>
                                    </td>
                                    <td>
                                        <%= pref_data[i].preference_id %>
                                    </td>
                                    <td>
                                        <%= pref_data[i].preference_location%>
                                    </td>
                                    <td>
                                        <%= pref_data[i].c_ctc%>
                                    </td>
                                    <td>
                                        <%= pref_data[i].e_ctc%>
                                    </td>
                                    <td>
                                        <%= pref_data[i].department%>
                                    </td>
                                    
                                    <% } %>
                        </tbody>
                      </table>           
</body>
</html>
